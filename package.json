{
    "name": "electron-jewel-thief",
    "version": "1.0.0",
    "description": "Electron Jewel Thief",
    "main": "main.js",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder --publish never",
        "start": "electron ."
    },
    "build": {
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thesourorange/neilstuff/electron-jewel-thief.git"
    },
    "keywords": [
        "Electron",
        "jewel",
        "thief"
    ],
    "author": "Neil Brittliff",
    "license": "CC0-1.0",
    "dependencies": {
        "mime": "^3.0.0",
        "jquery": "^3.6.0"
    },
    "devDependencies": {
        "electron": "^18.0.4",
        "electron-builder": "^23.0.3",
        "node": "^17.7.2"
    },
    "bugs": {
        "url": "https://github.com/thesourorange/electron-jewel-thief/issues"
    },
    "homepage": "https://github.com/thesourorange/electron-jewel-thief#readme"
}
